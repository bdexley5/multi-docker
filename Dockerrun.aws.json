{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
	  "name": "client",
	  "image": "bdexley5/multi-client",
	  "hostname": "client",
	  "essential": false,
	  "memory": 128
	},
    {
	  "name": "server",
	  "image": "bdexley5/multi-server",
	  "hostname": "api",
	  "essential": false,
	  "memory": 128
	},
    {
	  "name": "worker",
	  "image": "bdexley5/multi-worker",
	  "hostname": "worker",
	  "essential": false,
	  "memory": 128
	},
    {
	  "name": "nginx",
	  "image": "bdexley5/multi-nginx",
	  "essential": true,
	  "memory": 128,
	  "portMappings": [
	    {
	      "hostPort": 80,
		  "containerPort": 80
		}
	  ],
	  "links": [ "client", "server" ]
	}
  ]
}